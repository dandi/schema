        "dandi:HasPart",
        "dandi:IsReferencedBy",
        "dandi:References",
        "dandi:IsDocumentedBy",
        "dandi:Documents",
        "dandi:IsCompiledBy",
        "dandi:Compiles",
        "dandi:IsVariantFormOf",

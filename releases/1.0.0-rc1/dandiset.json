      "type": "object",
      "properties": {
        "identifier": {
          "title": "Identifier",
          "nskey": "schema",
          "minLength": 1,
          "maxLength": 2083,
          "format": "uri",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "description": "The name of the item.",
          "maxLength": 150,
          "nskey": "schema",
          "type": "string"
        },
        "schemaKey": {
          "title": "Schema Key",
          "default": "Anatomy",
          "readOnly": true,
          "type": "string",
          "const": "Anatomy"
        }
      }
    },
    "TypeModel": {
      "title": "Type Model",
      "description": "Base class for enumerated types",
      "type": "object",
      "properties": {
        "identifier": {
          "title": "Identifier",
          "nskey": "schema",
          "minLength": 1,
          "maxLength": 2083,
          "format": "uri",
          "type": "string"
        },
        "name": {
          "title": "Name",
          "description": "The name of the item.",
          "maxLength": 150,
          "nskey": "schema",
          "type": "string"
        },
        "schemaKey": {
          "title": "Schema Key",
          "default": "GenericType",
          "readOnly": true,
          "type": "string",
          "const": "GenericType"
        }
      }
    },
    "LicenseType": {
      "title": "LicenseType",
      "description": "An enumeration of supported licenses",
      "enum": [
        "dandi:CC0",
        "dandi:CCBY40",
        "dandi:CCBYNC40"
      ]
    },
    "EthicsApproval": {
      "title": "Ethics Approval",
      "description": "Information about ethics committee approval for project",
      "type": "object",
      "properties": {
        "identifier": {
          "title": "Approved protocol identifier",
